from(bucket: "WFDS")
  |> range(start: -1h)  // Select data from the last 1 hour
  |> filter(fn: (r) => r._measurement == "sensor_data")  // Filter by measurement
  |> filter(fn: (r) => r._field == "temperature" or r._field == "moisture" or r._field == "wind_speed" or r._field == "condition_count")  // Filter by fields (like SQL's SELECT columns)
  |> yield(name: "raw_data_last_hour")  // Return the raw data for the last hour (mimicking SQL SELECT)
